{
  "name": "Ride-My-Way",
  "version": "1.0.0",
  "description": "This a carpooling app where user offer rides to other users and other users can request to join ride",
  "main": "app.js",
  "scripts": {
    "postinstall": "npm run build:prod",
    "start:dev": "nodemon ./server/app.js --exec babel-node",
    "start": "node dist/app.js",
    "clean:dist": "rimraf ./dist",
    "build:prod": "npm run clean:dist && babel ./server --out-dir dist --copy-files",
    "test": " nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register server/test/test/index.test.js --timeout 10000ms",
    "test:jest": "jest client/tests",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "createDb": "psql -U postgres -f ./server/db/data.sql",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js",
    "start:concurrent": "concurrently \"npm run start:dev\" \"npm run dev\"",
    "build:client": "webpack --mode=production",
    "build": "npm run build:client && npm run build:prod",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/client/tests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/client/tests/__mocks__/styleMock.js"
    }
  },
  "engines": {
    "node": "10.4.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "rideMyWay"
  ],
  "author": "Valentine Ezeh",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "body-parser": "~1.17.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.0.1",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "faker": "^4.1.0",
    "file-loader": "^2.0.0",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^7.4.2",
    "mocha": "^3.5.0",
    "morgan": "~1.8.1",
    "nodemon": "^1.17.5",
    "pg": "^7.6.1",
    "pg-promise": "^5.9.7",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "shortid": "^2.2.13",
    "style-loader": "^0.23.0",
    "supertest": "^3.0.0",
    "validator": "^10.4.0"
  },
