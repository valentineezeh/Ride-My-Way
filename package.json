{
  "name": "Ride-My-Way",
  "version": "1.0.0",
  "description": "This a carpooling app where user offer rides to other users and other users can request to join ride",
  "main": "app.js",
  "scripts": {
    "postinstall": "npm run build:prod",
    "start:dev": "nodemon ./server/app.js --exec babel-node",
    "start": "node dist/app.js",
    "clean:dist": "rimraf ./dist",
    "build:prod": "npm run clean:dist && babel ./server --out-dir dist",
    "test": " nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register server/dummy_data/test.js --timeout 10000ms",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "engines": {
    "node": "10.4.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "rideMyWay"
  ],
  "author": "Valentine Ezeh",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.17.1",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "mocha": "^3.5.0",
    "morgan": "~1.8.1",
    "nodemon": "^1.17.5",
    "pg": "^7.4.3",
    "pg-promise": "^5.9.7",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "mocha-istanbul": "^0.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "coveralls": "^2.13.1",
    "nyc": "^11.1.0"
  }
}
